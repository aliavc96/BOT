<!-- reglaS para saber si el nombre que dice es el correcto-->

<category>
	<pattern><set>nombre</set> * </pattern>
	<template>
		<think>
			<set name="existe">
			<srai>FINDITEM <star index="2"/> IN <get name="list_nombres"/></srai>
			</set>
		</think>
		<condition name="existe">
			<li value="0">Ese nombre no pertenece al juego...</li>
			<li> pues... </li>
		</condition>

		<think>
			<set name="respuesta">
				<star index="2"/>
			</set>
		</think>
		<think>
		<set name="iguales">
			<srai>
			COMPARE <get name="elegida"/> WITH <get name="respuesta"/>
			</srai>
		</set>
		</think>
		<condition name="iguales">
			<li value="YES"> siiiiii!! Has acertado :D. Otra partidita?</li>
			<li> no, lo siento. Sigue intentadolo :3 </li>
		</condition>
</template>
</category>




