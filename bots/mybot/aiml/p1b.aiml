<!-- regla para saber si es mujer -->
<category>
<pattern>*</pattern>
<that>ES MUJER</that>
<template>

		<think>
			<set name="respuesta">
				<star/>
			</set>

		</think>
		<think>
			<set name="item">
			<srai> ITERATE <get name="candidatos"/> </srai>
			</set>
		</think>
		<condition name="item">
			<li value="end"></li>
			<li>
				<think>
					<set name="es">
						<map name="genero"><get name="item"/></map>
					</set>
				</think>
				<condition name="respuesta">
					<li value="si">
						
						<condition name="es">
							<li value="hombre">
								<think>
									<think>
										<set name="pos"> <!-- almacena la posicion donde se encuentra el nombre a borrar -->
											<srai>FINDITEM <get name="item"/> IN <get name="candidatos"/></srai>
										</set>
									</think>
									<set name="candidatos">
										<srai>
										REMOVEITEM <get name="pos"/> IN <get name="candidatos"/>
										</srai>
									</set>
								</think>

							</li>
						</condition>

					</li>
					<!-- si la respuesta que le damos es no -->
					<li value="no">
						<condition name="es">
							<li value="mujer"> <!-- borramos los que si tienen bigote -->

								<think>
									<set name="pos"> <!-- almacena la posicion donde se encuentra el nombre a borrar -->
										<srai>FINDITEM <get name="item"/> IN <get name="candidatos"/></srai>
									</set>
								</think>
								<think>
									<set name="candidatos">
										<srai>
										REMOVEITEM <get name="pos"/> IN <get name="candidatos"/>
										</srai>
									</set>
								</think>
							</li>
						</condition>
					</li>
				</condition>
				<think>
					<set name="item">
					<srai>NEXT</srai>
					</set>
				</think>
				<loop/>

			</li>
		</condition>
		de aucerdo...
		<condition name="respuesta">
					<li value="no">tiene bigote?</li>
					<li value="si">tiene el pelo blanco?</li>
		</condition>
	
</template>
</category>

